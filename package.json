{
  "name": "@aganitha/reporting-framework",
  "version": "1.0.4",
  "description": "Plugin-based framework for LLM-powered report generation",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "dev": "tsc -p tsconfig.json --watch",
    "clean": "rm -rf dist",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "keywords": [
    "framework",
    "plugin",
    "llm",
    "reports",
    "report-generation",
    "ai"
  ],
  "author": "Nikhil Tiwari",
  "dependencies": {
    "@ai-sdk/deepseek": "^1.0.29",
    "@ai-sdk/google": "^2.0.43",
    "@ai-sdk/openai": "^2.0.72",
    "ai": "^5.0.101",
    "csv-parse": "^6.1.0",
    "debug": "^4.4.3",
    "fast-xml-parser": "^5.3.2",
    "js-yaml": "^4.1.0",
    "marked": "^14.1.3",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.12",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^24.10.1",
    "@types/nunjucks": "^3.2.6",
    "playwright": "^1.57.0",
    "typescript": "^5.6.0",
    "vitest": "^4.0.12"
  },
  "publishConfig": {
    "access": "public"
  }
}
